                                    <div class="row row-30 mbn-40">

                                        <div class="col-xl-9 col-lg-8 col-12 order-1 order-lg-2 mb-40">
                                            <div class="row">
                                                <% for(var i=0; i < ItemDetails.length; i++) { %>
                                                <% if(ItemDetails[i].prescribed == 'false') { %>
                                                    <form action="view-item-process" method="post">
                                                        <div class="col-xl-4 col-md-6 col-12 mb-40">
                                                        <div class="col-md-6 col-12 mb-15"><input id="itemCode" name="itemCode" type="text" value="<%= ItemDetails[i].item_code %>" style="visibility:hidden"></div>
                                                        <div class="product-item border">
                                                            <div class="product-inner">
                                                                <div class="image">
                                                                    <img src="<%= ItemDetails[i].image %>" alt="" onerror=this.src="images/pharmacies/alt-store.jpg" style="background-size: 150px;
                                                                    width: 300px;
                                                                    height: 300px; object-fit: contain; align-self: center;">
                                                                    <div class="image-overlay">
                                                                        <div class="action-buttons">
                                                                            <% if(userFName.length > 0) { %>
                                                                                <button onsubmit="view-item-process">add to cart  <% userFName %></button>                                 
                                                                            <% } else { %>
                                                                                <button title="Please log in!" disabled>add to cart</button>    
                                                                            <% } %>
