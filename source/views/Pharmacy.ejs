<!doctype html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Medi2Door - Bringing the medicine at your door</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" type="image/x-icon" href="assets/images/favicon.ico">


    <link rel="stylesheet" href="assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="assets/css/icon-font.min.css">
    <link rel="stylesheet" href="assets/css/plugins.css">
    <link rel="stylesheet" href="assets/css/helper.css">
    <link rel="stylesheet" href="assets/css/style.css">
    <script src="assets/js/vendor/modernizr-3.7.1.min.js"></script>
</head>

<body>
    <div class="main-wrapper">

        <div class="header-section section">
            <div class="header-top header-top-one bg-theme-two">
                <div class="container-fluid">
                    <div class="row align-items-center justify-content-center">
                        <div class="col mt-10 mb-10 d-none d-md-flex">
                            <div class="header-top-left">
                                <p>Welcome to Medi2Door - Bringing the medicine at your door</p>
                            </div>
                        </div>
                        <div class="col mt-10 mb-10">
                            <div class="header-top-right">
                                <% if(userFName.length > 0) { %>
                                    <p>Hi <%= userFName %></p>                                        
                                    <p><a href="/my-account">My Account</a></p>
                                    <p><a href="/logout-process">Logout</a></p>
                                <% } else { %>
                                <p><a href="/login">Register</a><a href="/login">Login</a></p>
                                <% } %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="header-bottom header-bottom-one header-sticky">
                <div class="container-fluid">
                    <div class="row menu-center align-items-center justify-content-between">
                        <div class="col mt-15 mb-15">
                            <div class="header-logo">
                                <a href="/">
                                    <img src="assets/images/logo.png">
                                </a>
                            </div>
                        </div>
                        <div class="col order-2 order-lg-3">
                            <div class="header-shop-links">
                                <div class="header-mini-cart">
                                    <img src="assets/images/icons/cart.png" alt="Cart"> 
                                    <% if (totalPrice == 0) { %>
                                        <span>Rs.<%= totalPrice %></span>
                                    <% } else { %>
                                        <span>Rs.<%= totalPrice %></span>&nbsp;&nbsp;<button type="button" class="btn btn-success btn-sm"><a href="/cart">View Cart</a></button> 
                                    <% } %>               
                                </div>
                            </div>
                        </div>

                        <div class="col order-3 order-lg-2">
                            <div class="main-menu">
                                <nav>
                                    <ul>
                                        <li>
                                            <a class="active" href="/index">HOME</a>
                                        </li>
                                        <li>
                                            <a href="#">ABOUT</a>
                                        </li>
                                        <li><a href="contact.html">CONTACT</a></li>
                                    </ul>
                                </nav>
                            </div>
                        </div>

                        <!-- Mobile Menu -->
                        <div class="mobile-menu order-12 d-block d-lg-none col"></div>

                    </div>
                </div>
            </div>

        </div>
        <!-- Page Banner Section Start -->
        <div class="page-banner-section section" style="background-image: url(assets/images/page-cover.jpg)">
            <div class="container">
                <div class="row">
                    <div class="page-banner-content col">

                        <h1>Pharmacy</h1>
                        <ul class="page-breadcrumb">
                            <li><a href="/index">Medi2Door</a></li>
                            <li><a href="/view-supplier-details">Pharmacy</a></li>
                        </ul>

                    </div>
                </div>
            </div>
        </div><!-- Page Banner Section End -->
        <!-- Product Section Start -->
        <div class="product-section section pt-100">            
            <div class="container">
                <div class="col-12 mb-15"> 
                    <% if(message.length > 5) { %>
                        <div class="alert alert-success alert-dismissible fade show" role="alert">
                            <%= message %>
                            <button type="button" class="close" data-dismiss="alert" style="background-color: transparent;">
                            <span aria-hidden="true">&times;</span>
                            </button>
                        </div>                                  
                    <% } else if (message.length == 0) { %>
                        <span aria-hidden="true"></span>
                    <% } else if (message.length == 4) { %>
                        <span aria-hidden="true"></span>
                    <% } else { %>
                    <div class="alert alert-danger alert-dismissible fade show" role="alert">
                        <span>Something went wrong!</span>
                        <button type="button" class="close" data-dismiss="alert" style="background-color: transparent;">
                        <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <% } %>
                </div>

                <div class="row row-20 mb-10">
                    <div class="col-lg-4 col-12 mb-40">
                        <img src="<%= SupplierDetails.storeImage %>" alt="" onerror=this.src="images/pharmacies/alt-store.jpg" style="background-size: 150px;
                        width: 300px;
                        height: 300px; object-fit: contain; align-self: center;"> 
                    </div>

                    <div class="col-lg-8 col-12 mb-40">
                        <div class="single-product-content">
                            <div class="head">
                                <div class="head-left">
                                    <h2 class="title"> <%= SupplierDetails.storeDescription %></h3>
                                </div>

                                <div class="head-right">
                                    <h5 class="nmra bold">NMRA : <%= SupplierDetails.nmraRegistration %></h5>
                                    <h5 class="id bold">Pharmacy ID: <%= SupplierDetails.pharmacistRegistration %></h5>
                                </div>
                            </div>
                           
                            <h4 class="name">Supplier Name: <span><%= SupplierDetails.firstName %> <%= SupplierDetails.lastName %> </span></span></h4>
                            <h4 class="email">Email: <span> <%= SupplierDetails.email %> </span></span></h4>
                            <br>
                            <br>
                            <span class="availability">Location: <span><%= SupplierDetails.street %> - <%= SupplierDetails.city %> </span></span>
                        </div>
                    </div>
                </div>

                <div class="row mb-50">
                    <!-- Tab panes -->
                    <!-- Landing -->
                    <div class="tab-content col-12" id="landing">
                    <!-- Nav tabs -->
                    <div class="col-12">
                        <ul class="pro-info-tab-list section nav">
                            <li><a class="active" href="#NonPrescribed" data-toggle="tab">Non-Prescribed</a></li>
                            <li><a href="#Prescribed" data-toggle="tab">Prescribed</a></li>
                        </ul>
                    </div>
                        <div class="tab-pane active" id="NonPrescribed">
