                        <div class="col-lg-2 col-12 mb-30">
                            <p>Status :</p>
                        </div>
                        <div class="col-lg-4 col-12 mb-30">
                            <label class="bold">Pending</label>
                        </div>
                        <div class="col-lg-2 col-12 mb-30">
                            <p>Total Amount (Rs) :</p>
                        </div>
                        <div class="col-lg-4 col-12 mb-30">
                            <label class="bold">1200.00</label>
                        </div>
                    </div>
                    <div class="row mt-2">
                        <div class="col-lg-2 col-12 mb-30">
                            <p>Prescription :</p>
                        </div>
                        <div class="col-lg-4 col-12 mb-30">
                            <button class="btn btn-primary-cs btn-round">View Prescription</button>
                        </div>
                    </div>

                    <table class="table table-bordered">
                        <thead class="thead-light text-center">
                            <tr>
                                <th>No</th>
                                <th>Image</th>
                                <th>Product</th>
                                <th>Unit Price</th>
                                <th>Quantity</th>
                                <th>Total Amount</th>
